{
  "name": "goddess-hat",
  "version": "0.0.1",
  "private": true,
  "description": "基于Tarojs框架的小程序业务框架",
  "templateInfo": {
    "name": "redux",
    "typescript": false,
    "css": "stylus"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "SERVER_ENV=yz MOCK=true npm run dev:weapp",
    "release": "SERVER_ENV=prod NODE_ENV=production npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "node boot.js && npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "node boot.js && npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch"
  },
  "author": "shenghanqin",
  "license": "MIT",
  "dependencies": {
    "@tarojs/cli": "2.0.3",
    "@tarojs/components": "2.0.3",
    "@tarojs/components-qa": "2.0.3",
    "@tarojs/redux": "2.0.3",
    "@tarojs/redux-h5": "2.0.3",
    "@tarojs/router": "2.0.3",
    "@tarojs/taro": "2.0.3",
    "@tarojs/taro-alipay": "2.0.3",
    "@tarojs/taro-h5": "2.0.3",
    "@tarojs/taro-qq": "2.0.3",
    "@tarojs/taro-quickapp": "2.0.3",
    "@tarojs/taro-swan": "2.0.3",
    "@tarojs/taro-tt": "2.0.3",
    "@tarojs/taro-weapp": "2.0.3",
    "@tarojs/with-weapp": "2.0.3",
    "babel-runtime": "^6.26.0",
    "nerv-devtools": "^1.5.5",
    "nervjs": "^1.5.5",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-cropper": "^1.0.9",
    "tencentcloud-sdk-nodejs": "^3.0.117"
  },
  "devDependencies": {
    "@tarojs/mini-runner": "2.0.3",
    "@tarojs/plugin-babel": "2.0.3",
    "@tarojs/plugin-csso": "2.0.3",
    "@tarojs/plugin-stylus": "2.0.3",
    "@tarojs/plugin-uglifyjs": "2.0.3",
    "@tarojs/webpack-runner": "2.0.3",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "eslint-config-taro": "2.0.3",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "2.0.3",
    "eslint-plugin-typescript": "^0.14.0",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.0.3",
    "stylelint-taro-rn": "2.0.3",
    "typescript": "^3.0.1"
  }
}
